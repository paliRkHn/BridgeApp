{
  "expo": {
    "name": "Bridge02",
    "slug": "Bridge02",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "bridgeapp",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.palomarojas.bridgeapp",
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "bundleIdentifier": "com.palomarojas.bridgeapp",
      "edgeToEdgeEnabled": true,
      "package": "com.palomarojas.bridgeapp",
      "intentFilters": [
        {
          "action": "VIEW",
          "category": ["DEFAULT","BROWSABLE"],
          "data": [
            {
              "scheme": "bridgeapp"
            }
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
